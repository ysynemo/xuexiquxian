###################################################################################################################
################链接内数据留作后续备用，数据来源联合国网站：data.un.org/en/index.html##################################
###################################################################################################################
import linkGrabber，re

links = linkGrabber.Links('http://data.un.org/en/index.html')           
gb = links.find(href=re.compile("iso"),pretty=False)
n = len(gb)
for i in range(n):
    url = gb[i]['href']
    print(url)
    with open("result1.txt", 'a+',encoding='utf8',errors='ignore') as f:
        f.write(url+'\n')                

