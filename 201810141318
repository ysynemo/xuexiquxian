def get_domain(word):
    try:    
        links = linkGrabber.Links(word)
        try:           
            gb = links.find(href=re.compile("twitter.com"),pretty=False)
            n = len(gb)
            for i in range(n):
                url = gb[i]['href']
                print(url)
            with open("D:/B-å¤§è¿ç†å·¥å¤§å­¦æœŸé—´ææ–™/1.0-æœŸåˆŠæ¨ç‰¹å…³æ³¨ç½‘ç»œ/1.0.0---SCIæ”¶å½•æœŸåˆŠ2017çš„å‡ºç‰ˆå•†Twitterå…³æ³¨ç½‘ç»œ/google_selenium_å‡ºç‰ˆå•†å®˜ç½‘_list/é“¾æ¥_twitter.txt", 'a',encoding='utf8',errors='ignore') as f:
                if n != 0:
                    f.write('æºï¼š{}\té“¾æ¥ï¼š{}\n'.format(word,url))
                else:
                    f.write('æºï¼š{}\té“¾æ¥ï¼š{}\n'.format(word +"æ²¡æœ‰twitteré“¾æ¥"))
        except ConnectionError:
            pass
    except:
        pass
        print("è·å–twitteré“¾æ¥å¤±è´¥")
        with open("D:/B-å¤§è¿ç†å·¥å¤§å­¦æœŸé—´ææ–™/1.0-æœŸåˆŠæ¨ç‰¹å…³æ³¨ç½‘ç»œ/1.0.0---SCIæ”¶å½•æœŸåˆŠ2017çš„å‡ºç‰ˆå•†Twitterå…³æ³¨ç½‘ç»œ/google_selenium_å‡ºç‰ˆå•†å®˜ç½‘_list/é“¾æ¥_twitter.txt", 'a',encoding='utf8',errors='ignore') as f:
            f.write('æºï¼š{}\té“¾æ¥ï¼š{}\n'.format(word,"è·å–twitteré“¾æ¥å¤±è´¥"))

def get_txt_links():
    keywords=[]
    with open("D:/B-å¤§è¿ç†å·¥å¤§å­¦æœŸé—´ææ–™/1.0-æœŸåˆŠæ¨ç‰¹å…³æ³¨ç½‘ç»œ/1.0.0---SCIæ”¶å½•æœŸåˆŠ2017çš„å‡ºç‰ˆå•†Twitterå…³æ³¨ç½‘ç»œ/google_selenium_å‡ºç‰ˆå•†å®˜ç½‘_list/é“¾æ¥.txt", 'r',encoding='utf8',errors='ignore') as f:
        for line in f.readlines():
            if line.strip().startswith('http'):
                keywords.append(line.strip())
        return keywords
        
for each in get_txt_links():
    get_domain(each)

