import tweepy,time
from tweepy import OAuthHandler, API
import requests

def getAPI():
    consumer_key = 'xt602vTCI6dkBEZTyve'
    consumer_secret = 'yCURoYZLGR0wg8KAPFynUBDAVMHjPWqtRGv3UG'
    access_token = '851972452LLNRSLCWDRyivNUOimhGqXVt5YUc'
    access_secret = 'Dymg3Az4uBZ5XTtFrcKKi8CJb6OmNNaUqUy'
    auth = tweepy.OAuthHandler(consumer_key, consumer_secret)
    auth.set_access_token(access_token, access_secret)
    print ('success')
    api = tweepy.API(auth)
    return api
def getUserInfo(api,q,mode = 'search'):
    if mode == 'search':#é‡‡ç”¨æœç´¢æ¨¡å¼è¿›è¡Œç”¨æˆ·ä¿¡æ¯é‡‡é›†
        users = api.search_users(q, 1, 1)
        if len(users) == 0:
            print('user does not exist')
            return None
        else:
            user = users[0]._json
    else:#é‡‡ç”¨ç›´æ¥è·å–ç”¨æˆ·ä¿¡æ¯çš„æ¨¡å¼è·å–
        user = api.get_user(q)  
    id = user['id']
    screen_name = user['screen_name']
    return str(id), screen_name


def limit_handled(cursor):
    while True:
        try:
            yield cursor.next()
        except tweepy.RateLimitError:
            time.sleep(15 * 60)


if __name__ == "__main__":
    api = getAPI()
    f = open('åŒ»å­¦æœŸåˆŠ.txt','r')
    f_out = open('åŒ»å­¦æœŸåˆŠ_result.txt','w')
    for line in f:
        line = line.strip()
        query = line
        result = getUserInfo(api,query)
        if result:
            print('#'.join(result))
            f_out.write(u'#'.join(result)+u'\n')
        else:
            f_out.write('do not exist'+'\n')
    f_out.close()

