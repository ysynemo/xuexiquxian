import time
import linkGrabber
import re
from selenium import webdriver


user_agent = ("Mozilla/5.0 (Windows NT 10.0; WOW64) "
            "AppleWebKit/537.36 (KHTML, like Gecko) "
            "Chrome/56.0.2924.87 Safari/537.36")

fr = open('D:/B-å¤§è¿ç†å·¥å¤§å­¦æœŸé—´ææ–™/1.0-æœŸåˆŠæ¨ç‰¹å…³æ³¨ç½‘ç»œ/1.0.0---SCIæ”¶å½•æœŸåˆŠ2017çš„å‡ºç‰ˆå•†Twitterå…³æ³¨ç½‘ç»œ/google_selenium_å‡ºç‰ˆå•†å®˜ç½‘_list/å‡ºç‰ˆå•†.txt', encoding='utf-8')
data = fr.readlines()
keywords = []
for d in data:
    d = d.replace('\n','')
    keywords.append(d)

try:
##############################################################
###############è·å–åˆå§‹é¡µé¢####################################
##############################################################
    for word in keywords:   #è°ƒç”¨webdriver
        chrome_path = r"C:\Program Files (x86)\Google\Chrome\Application\chromedriver.exe"
        driver = webdriver.Chrome(chrome_path)
        driver.get("https://www.google.com")
        driver.implicitly_wait(1)
        time.sleep(1)
        try:    #è¾“å…¥å…³é”®è¯æ£€ç´¢
            driver.find_element_by_class_name('gsfi').clear
            driver.find_element_by_class_name('gsfi').send_keys(word)
            driver.find_element_by_xpath("""//*[@id="tsf"]/div[2]/div[3]/center/input[1]""").click()
            driver.implicitly_wait(5)
#############################################################
###############è§£æäºŒçº§é¡µé¢###################################
#############################################################
        
            info = driver.find_element_by_class_name('r')  #è·å–å¤´æ¡å†…å®¹ä¿å­˜
            link = info.find_element_by_tag_name('a').get_attribute('href')
            text = info.find_element_by_tag_name('h3').text
            with open('D:/B-å¤§è¿ç†å·¥å¤§å­¦æœŸé—´ææ–™/1.0-æœŸåˆŠæ¨ç‰¹å…³æ³¨ç½‘ç»œ/1.0.0---SCIæ”¶å½•æœŸåˆŠ2017çš„å‡ºç‰ˆå•†Twitterå…³æ³¨ç½‘ç»œ/google_selenium_å‡ºç‰ˆå•†å®˜ç½‘_list/å‡ºç‰ˆå•†_twitter.txt','a',encoding='utf8',errors='ignore') as f:
                f.write('æºï¼š{}\tæ ‡é¢˜ï¼š{}\té“¾æ¥ï¼š{}\n'.format(word,text,link))
            driver.close()
        except:
